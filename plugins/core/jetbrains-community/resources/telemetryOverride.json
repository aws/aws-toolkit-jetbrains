        {
            "name": "cwsprChatActiveEditorImportCount",
            "type": "int",
            "description": "Number of import statements in the active editor"
        },
        {
            "name": "cwsprChatResponseCodeSnippetCount",
            "type": "int",
            "description": "Number of code snippets in response"
