<idea-plugin>
    <extensions defaultExtensionNs="com.intellij">
        <codeInsight.lineMarkerProvider language="JAVA" implementationClass="software.aws.toolkits.jetbrains.services.lambda.JavaLambdaLineMarker"/>
    </extensions>

    <extensions defaultExtensionNs="aws.toolkit">
        <lambdaPackager language="JAVA" implementationClass="software.aws.toolkits.jetbrains.services.lambda.JavaLambdaPackager"/>
        <lambda.handlerResolver runtime="JAVA" implementation="software.aws.toolkits.jetbrains.services.lambda.JavaLambdaHandlerResolver"/>
    </extensions>
</idea-plugin>